ans ces conditions particuli\u00e8res.<\/p>\n"}]},"_links":{"self":[{"href":"https:\/\/territoireengagetransitionecologique.ademe.fr\/wp-json\/wp\/v2\/posts\/1800"}],"collection":[{"href":"https:\/\/territoireengagetransitionecologique.ademe.fr\/wp-json\/wp\/v2\/posts"}],"about":[{"href":"https:\/\/territoireengagetransitionecologique.ademe.fr\/wp-json\/wp\/v2\/types\/post"}],"author":[{"embeddable":true,"href":"https:\/\/territoireengagetransitionecologique.ademe.fr\/wp-json\/wp\/v2\/users\/5"}],"replies":[{"embeddable":true,"href":"https:\/\/territoireengagetransitionecologique.ademe.fr\/wp-json\/wp\/v2\/comments?post=1800"}],"version-history":[{"count":3,"href":"https:\/\/territoireengagetransitionecologique.ademe.fr\/wp-json\/wp\/v2\/posts\/1800\/revisions"}],"predecessor-version":[{"id":1804,"href":"https:\/\/territoireengagetransitionecologique.ademe.fr\/wp-json\/wp\/v2\/posts\/1800\/revisions\/1804"}],"wp:featuredmedia":[{"embeddable":true,"href":"https:\/\/territoireengagetransitionecologique.ademe.fr\/wp-json\/wp\/v2\/media\/1801"}],"wp:attachment":[{"href":"https:\/\/territoireengagetransitionecologique.ademe.fr\/wp-json\/wp\/v2\/media?parent=1800"}],"wp:term":[{"taxonomy":"category","embeddable":true,"href":"https:\/\/territoireengagetransitionecologique.ademe.fr\/wp-json\/wp\/v2\/categories?post=1800"},{"taxonomy":"post_tag","embeddable":true,"href":"https:\/\/territoireengagetransitionecologique.ademe.fr\/wp-json\/wp\/v2\/tags?post=1800"}],"curies":[{"name":"wp","href":"https:\/\/api.w.org\/{rel}","templated":true}]}}